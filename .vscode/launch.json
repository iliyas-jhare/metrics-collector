{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: main.py",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/src/main.py",
            "console": "integratedTerminal",
            "args": "--metrics-source-path C:/Metrics/Source --metrics-destination-path C:/Metrics/Destination"
        }
    ]
}